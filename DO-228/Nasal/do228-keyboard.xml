          <key n="77">
                <name>M</name>
                <desc>Condition leaner</desc>
                <repeatable type="bool">true</repeatable>
                <binding>
                    <command>property-adjust</command>
                    <property>controls/engines/engine[0]/condition</property>
                    <step>-0.01</step>
                    <min>0</min>
                    <max>1</max>
                    <wrap>0</wrap>
                </binding>
                <binding>
                    <command>property-adjust</command>
