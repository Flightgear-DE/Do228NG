<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>strobe.ac</path>

  <animation>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
    <object-name>info</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
    <object-name>info</object-name>
    <emission>
	<factor-prop>/controls/lighting/panel-norm</factor-prop>
      <red>   0.1 </red>
      <green> 0.04 </green>
      <blue>  0.04 </blue>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>info</object-name>
    <object-name>Switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/strobe</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/switch</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Switch</object-name>
    <property>/controls/lighting/strobe</property>
    <factor> -40 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


